<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="cashadvance_seq" model="ir.sequence">
      <field name="name">Appointment</field>
      <field name="padding">3</field>
      <field name="code">appointment.number</field>
      <field name="prefix">OPT/</field>
    </record>

    <record id="increment_test_sequence" model="ir.sequence">
      <field name="name">test_id</field>
      <field name="code">increment_test</field>
      <field name="prefix">ANA</field>
      <field name="padding">3</field>
    </record>

    <record id="increment_xr_sequence" model="ir.sequence">
      <field name="name">xr_id</field>
      <field name="code">increment_xr</field>
      <field name="prefix">RXD</field>
      <field name="padding">3</field>
    </record>

    <record id="increment_eco_sequence" model="ir.sequence">
      <field name="name">eco_id</field>
      <field name="code">increment_echography</field>
      <field name="prefix">ECO</field>
      <field name="padding">3</field>
    </record>

    <record id="increment_cito_sequence" model="ir.sequence">
      <field name="name">cito_id</field>
      <field name="code">increment_citology</field>
      <field name="prefix">CIT</field>
      <field name="padding">3</field>
    </record>
  </data>  


</odoo>